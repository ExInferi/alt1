{
	"compilerOptions": {
		"moduleResolution": "node",
		"module": "commonjs",
		"lib": [ "es6", "dom" ],
		"target": "es6",
		"sourceMap": true,
		"alwaysStrict": true,
		"jsx": "react",
		"allowJs": false,
		"baseUrl": ".",
		"strictNullChecks": false,
		"strictPropertyInitialization": false,
		"experimentalDecorators": true,
		"paths": {
			"@alt1/*": [ "alt1/*" ]
		}
	},
	"exclude": [ "node_modules", "types", "**/dist" ],
	"compileOnSave": false
}